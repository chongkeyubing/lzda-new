  <!--分页查询-->
  <select id="selectForPage" parameterType="com.mwkj.lzda.model.RptResponsibilityPerform" resultType="com.mwkj.lzda.model.RptResponsibilityPerform">
    select t1.* from rpt_responsibility_perform t1
    <where>
      <if test="organizationId!=null and organizationId!=''">
        t1.organization_id like '%${organizationId}%'
      </if>
    </where>
    order by t1.create_time desc
  </select>
