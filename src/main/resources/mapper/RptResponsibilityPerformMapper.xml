  <select id="selectCount" parameterType="com.mwkj.lzda.model.RptResponsibilityPerform" resultType="java.lang.Long">
    select count(*) from rpt_responsibility_perform t1
    <where>
      <if test="organizationId!=null and organizationId!=''">
        t1.organization_id like '%${organizationId}%'
      </if>
    </where>
  </select>



